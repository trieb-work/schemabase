{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": false,
    "noPropertyAccessFromIndexSignature": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@eci/adapters/braintree": ["libs/adapters/braintree/src/index.ts"],
      "@eci/adapters/easypost": ["libs/adapters/easypost/src/index.ts"],
      "@eci/adapters/lexoffice": ["libs/adapters/lexoffice/src/index.ts"],
      "@eci/adapters/mailchimp": ["libs/adapters/mailchimp/src/index.ts"],
      "@eci/adapters/package-trackers": [
        "libs/adapters/package-trackers/src/index.ts"
      ],
      "@eci/adapters/printer": ["libs/adapters/printer/src/index.ts"],
      "@eci/adapters/saleor/api": ["libs/adapters/saleor/api/src/index.ts"],
      "@eci/adapters/saleor/validation": [
        "libs/adapters/saleor/validation/src/index.ts"
      ],
      "@eci/context": ["libs/context/src/index.ts"],
      "@eci/data-access/prisma": ["libs/data-access/prisma/src/index.ts"],
      "@eci/events": ["libs/events/src/index.ts"],
      "@eci/http": ["libs/http/src/index.ts"],
      "@eci/integrations/product-data-feed": [
        "libs/integrations/product-data-feed/src/index.ts"
      ],
      "@eci/integrations/saleor-to-zoho": [
        "libs/integrations/saleor-to-zoho/src/index.ts"
      ],
      "@eci/integrations/strapi-orders-to-zoho": [
        "libs/integrations/strapi-orders-to-zoho/src/index.ts"
      ],
      "@eci/integrations/zoho/logistics": [
        "libs/integrations/zoho/logistics/src/index.ts"
      ],
      "@eci/util": ["libs/util/src/index.ts"],
      "@eci/util/constants": ["libs/util/constants/src/index.ts"],
      "@eci/util/errors": ["libs/util/errors/src/index.ts"],
      "@eci/util/ids": ["libs/util/ids/src/index.ts"],
      "@eci/util/logger": ["libs/util/logger/src/index.ts"],
      "@eci/util/retry": ["libs/util/retry/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
